{
  "name": "sora-auto-queue-prompts",
  "displayName": "Sora Auto Queue Prompts",
  "version": "2.1.0",
  "description": "Automate prompt generation and queue management for Sora AI video/image generation",
  "author": "YosefHayim",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YosefHayim/extension-sora-auto-queue-prompts.git"
  },
  "bugs": {
    "url": "https://github.com/YosefHayim/extension-sora-auto-queue-prompts/issues"
  },
  "homepage": "https://github.com/YosefHayim/extension-sora-auto-queue-prompts#readme",
  "keywords": [
    "sora",
    "ai",
    "prompts",
    "automation",
    "chrome-extension",
    "openai",
    "queue",
    "video-generation",
    "image-generation"
  ],
  "scripts": {
    "dev": "wxt",
    "build": "wxt build",
    "build:prod": "wxt build --mode production",
    "package": "wxt zip",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "clean": "del-cli .output build dist *.zip",
    "clean:all": "del-cli .output build dist *.zip && rm -rf .output node_modules/.cache",
    "prepare": "husky && wxt prepare",
    "postinstall": "wxt prepare",
    "lint": "prettier --check \"src/**/*.{ts,tsx}\" \"entrypoints/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"entrypoints/**/*.{ts,tsx}\""
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/chrome": "^0.0.258",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.22",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cpy-cli": "^5.0.0",
    "del-cli": "^5.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "wxt": "^0.20.11"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "react-icons": "^5.5.0",
    "task-master-ai": "^0.32.2"
  }
}
